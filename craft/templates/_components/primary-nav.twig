<nav class="c-primary-nav">
  <ul class="c-primary-nav__list  o-list-bare  o-list-inline">
    {% for block in primaryNav.primaryNav %}
      <li class="o-list-inline__item  c-primary-nav__item">
        <a href="{{ block.url }}" class="c-primary-nav__link{% if entry is defined and entry.section.handle == block.section.handle %}  active{% endif %}">{{ block.title }}</a>
      </li>
    {% endfor %}
  </ul>
</nav>
